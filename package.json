{
  "name": "nodejs-boilerplate",
  "author": "Pyroarsonist <velikiydan@gmail.com>",
  "license": "MIT",
  "version": "1.1.0",
  "description": "Node.js boilerplate",
  "main": "main.js",
  "keywords": [
    "boilerplate",
    "node",
    "js",
    "babel"
  ],
  "scripts": {
    "start": "DEBUG=app:* nodemon --exec babel-node src/main.js",
    "lint": "eslint . --ignore-path .gitignore --fix",
    "build": "rm -rf build && babel src -d build --ignore .gitignore --source-maps",
    "start:prod": "node -r @babel/register ./build/main.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/eslint-plugin": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/register": "^7.18.9",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-pyroarsonist": "^0.0.2",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "dotenv": "^8.2.0"
  }
}
